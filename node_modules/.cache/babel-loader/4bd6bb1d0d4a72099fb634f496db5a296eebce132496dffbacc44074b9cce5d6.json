{"ast":null,"code":"import gsap from 'gsap';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      isShuffling: true,\n      images: [],\n      selectedImage: null,\n      interval: null\n    };\n  },\n  created() {\n    setTimeout(() => {\n      axios.get('figurinhas.json').then(res => {\n        this.images = res.data;\n        this.startShuffling();\n      }).catch(error => {\n        console.error(\"Erro ao carregar o JSON:\", error);\n      });\n    }, 1000);\n  },\n  mounted() {\n    const bala = document.querySelector('#bala');\n    bala.addEventListener('click', () => {\n      gsap.to('#lata-e-bala', {\n        y: \"50vh\",\n        duration: 1,\n        ease: 'power4.inOut'\n      });\n    });\n  },\n  methods: {\n    startShuffling() {\n      if (this.images.length === 0) return;\n      this.isShuffling = true;\n      this.interval = setInterval(() => {\n        if (this.isShuffling) {\n          const nextIndex = Math.floor(Math.floor * this.images.length);\n          this.selectedImage = this.images[nextIndex].image;\n        }\n      }, 100);\n    },\n    stopShuffling() {\n      this.isShuffling = false;\n      clearInterval(this.interval);\n    }\n  }\n};","map":{"version":3,"names":["gsap","axios","data","isShuffling","images","selectedImage","interval","created","setTimeout","get","then","res","startShuffling","catch","error","console","mounted","bala","document","querySelector","addEventListener","to","y","duration","ease","methods","length","setInterval","nextIndex","Math","floor","image","stopShuffling","clearInterval"],"sources":["/Users/alexandrebeltramini/RepositoriosGit/baly-freegels/src/components/HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"heading-1\">\n    <img id=\"hand\" src=\"../assets/images/hand.png\" />\n    <h6> Toque na <span>Bala</span> para ganhar a sua figurinha!</h6>\n  </div>\n\n  <div class=\"sorteio-imagens\" @click=\"stopShuffling\">\n    <img v-if=\"selectedImage\" :src=\"selectedImage\" alt=\"Imagem sorteada\" />\n  </div>\n\n  <div id=\"lata-e-bala\">\n    <img id=\"lata\" src=\"../assets/images/lata.png\">\n    <img id=\"bala\" src=\"../assets//images/bala.png\">\n  </div>\n\n</template>\n\n<script>\nimport gsap from 'gsap';\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      isShuffling: true,\n      images: [],\n      selectedImage: null,\n      interval: null\n    }\n  },\n  created() {\n    setTimeout(()=>{\n      axios.get('figurinhas.json')\n      .then(res => {\n        this.images = res.data;\n        this.startShuffling();\n      })\n      .catch(error => {\n        console.error(\"Erro ao carregar o JSON:\", error);\n      });\n    },1000)\n  },\n  mounted() {\n    const bala = document.querySelector('#bala');\n\n    bala.addEventListener('click', () => {\n      gsap.to('#lata-e-bala', {\n        y: \"50vh\",\n        duration: 1,\n        ease: 'power4.inOut'\n      })\n\n    })\n  },\n\n  methods: {\n    startShuffling() {\n      if (this.images.length === 0) return;\n\n      this.isShuffling = true;\n      this.interval = setInterval(() => {\n        if (this.isShuffling) {\n          const nextIndex = Math.floor(Math.floor * this.images.length);\n          this.selectedImage = this.images[nextIndex].image;\n        }\n      }, 100);\n    },\n\n    stopShuffling() {\n      this.isShuffling = false;\n      clearInterval(this.interval);\n    }\n  }\n}\n\n</script>\n\n<style lang=\"scss\">\n.heading-1 {\n  background-color: white;\n  padding: 20px;\n  margin-bottom: 15px;\n  display: flex;\n  align-items: start;\n  border-radius: 20px;\n  gap: 10px;\n\n  span {\n    color: #7E1416\n  }\n\n  #hand {\n    max-width: 24px;\n    object-fit: contain;\n  }\n\n  h6 {\n    font-size: 20px !important;\n    max-width: 240px;\n    text-align: left;\n    margin: 0;\n  }\n}\n\n#lata-e-bala {\n  position: relative;\n\n}\n\n#lata {\n\n  max-height: 80vh;\n}\n\n#bala {\n  max-height: 52vh;\n  position: absolute;\n  bottom: 13px;\n  left: 50%;\n  transform: translatex(-50%);\n  transition: filter .3s;\n  cursor: pointer;\n\n  &:hover {\n    -webkit-filter: drop-shadow(1px 1px 1px white);\n    filter: drop-shadow(1px 1px 1px white);\n  }\n}\n</style>"],"mappings":"AAkBA,OAAOA,IAAG,MAAO,MAAM;AACvB,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE,IAAI;MACnBC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRC,UAAU,CAAC,MAAI;MACbP,KAAK,CAACQ,GAAG,CAAC,iBAAiB,EAC1BC,IAAI,CAACC,GAAE,IAAK;QACX,IAAI,CAACP,MAAK,GAAIO,GAAG,CAACT,IAAI;QACtB,IAAI,CAACU,cAAc,CAAC,CAAC;MACvB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD,CAAC,CAAC;IACJ,CAAC,EAAC,IAAI;EACR,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,MAAMC,IAAG,GAAIC,QAAQ,CAACC,aAAa,CAAC,OAAO,CAAC;IAE5CF,IAAI,CAACG,gBAAgB,CAAC,OAAO,EAAE,MAAM;MACnCpB,IAAI,CAACqB,EAAE,CAAC,cAAc,EAAE;QACtBC,CAAC,EAAE,MAAM;QACTC,QAAQ,EAAE,CAAC;QACXC,IAAI,EAAE;MACR,CAAC;IAEH,CAAC;EACH,CAAC;EAEDC,OAAO,EAAE;IACPb,cAAcA,CAAA,EAAG;MACf,IAAI,IAAI,CAACR,MAAM,CAACsB,MAAK,KAAM,CAAC,EAAE;MAE9B,IAAI,CAACvB,WAAU,GAAI,IAAI;MACvB,IAAI,CAACG,QAAO,GAAIqB,WAAW,CAAC,MAAM;QAChC,IAAI,IAAI,CAACxB,WAAW,EAAE;UACpB,MAAMyB,SAAQ,GAAIC,IAAI,CAACC,KAAK,CAACD,IAAI,CAACC,KAAI,GAAI,IAAI,CAAC1B,MAAM,CAACsB,MAAM,CAAC;UAC7D,IAAI,CAACrB,aAAY,GAAI,IAAI,CAACD,MAAM,CAACwB,SAAS,CAAC,CAACG,KAAK;QACnD;MACF,CAAC,EAAE,GAAG,CAAC;IACT,CAAC;IAEDC,aAAaA,CAAA,EAAG;MACd,IAAI,CAAC7B,WAAU,GAAI,KAAK;MACxB8B,aAAa,CAAC,IAAI,CAAC3B,QAAQ,CAAC;IAC9B;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}