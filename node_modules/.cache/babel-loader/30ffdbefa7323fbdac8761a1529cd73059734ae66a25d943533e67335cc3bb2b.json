{"ast":null,"code":"export default {\n  methods: {\n    closeShare() {\n      this.showShareOptions = false;\n      console.log('share Closed!');\n    },\n    shareSticker() {\n      if (!this.stickerText) {\n        alert(\"Ainda não há uma figurinha para compartilhar!\");\n        return;\n      }\n      const texto = `Eu tirei ${this.stickerText}, qual você vai tirar?`;\n      const url = window.location.href;\n      if (navigator.share && typeof navigator.share === \"function\") {\n        navigator.share({\n          title: \"Compartilhar figurinha\",\n          text: texto,\n          url: url\n        }).then(() => console.log(\"Compartilhamento concluído\")).catch(error => console.error(\"Erro ao compartilhar:\", error));\n      } else {\n        this.showShareOptions = true;\n      }\n    },\n    copiaCola() {\n      const texto = `Eu tirei ${this.stickerText}, qual você vai tirar?`;\n      const url = window.location.href;\n      const tempInput = document.createElement(\"input\");\n      tempInput.value = `${texto} ${url}`;\n      document.body.appendChild(tempInput);\n      tempInput.select();\n      document.execCommand(\"copy\");\n      document.body.removeChild(tempInput);\n      alert(\"Link copiado para a área de transferência!\");\n      this.closeShare();\n    }\n  }\n};","map":{"version":3,"names":["methods","closeShare","showShareOptions","console","log","shareSticker","stickerText","alert","texto","url","window","location","href","navigator","share","title","text","then","catch","error","copiaCola","tempInput","document","createElement","value","body","appendChild","select","execCommand","removeChild"],"sources":["/Users/alexandrebeltramini/RepositoriosGit/baly-freegels/src/components/ShareButton.vue"],"sourcesContent":["<template>\n    <div class=\"share-options\">\n        <h6>Compartilhe em suas redes!</h6>\n        <div class=\"share-buttons\">\n            <a :href=\"'https://api.whatsapp.com/send?text=' + encodeURIComponent(shareText + ' ' + shareUrl)\"\n                target=\"_blank\">\n                <img src=\"../assets/images/social/whatsapp.png\" alt=\"whatsapp\">\n            </a>\n            <a :href=\"'https://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent(shareUrl)\" target=\"_blank\">\n                <img src=\"../assets/images/social/facebook.png\" alt=\"facebook\">\n            </a>\n            <a :href=\"'https://api.whatsapp.com/send?text=' + encodeURIComponent(shareText + ' ' + shareUrl)\"\n                target=\"_blank\">\n                <img src=\"../assets/images/social/instagram.png\" alt=\"instagram\">\n            </a>\n            <a :href=\"'https://twitter.com/intent/tweet?text=' + encodeURIComponent(shareText) + '&url=' + encodeURIComponent(shareUrl)\"\n                target=\"_blank\">\n                <img src=\"../assets/images/social/twitter.png\" alt=\"twitter\">\n            </a>\n            <a @click=\"copiaCola\">\n                <img src=\"../assets/images/social/layer.png\" alt=\"copy-paste\">\n            </a>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    methods: {\n        closeShare() {\n      this.showShareOptions = false;\n      console.log('share Closed!')\n    },\n        shareSticker() {\n            if (!this.stickerText) {\n                alert(\"Ainda não há uma figurinha para compartilhar!\");\n                return;\n            }\n\n            const texto = `Eu tirei ${this.stickerText}, qual você vai tirar?`;\n            const url = window.location.href;\n\n            if (navigator.share && typeof navigator.share === \"function\") {\n                navigator.share({\n                    title: \"Compartilhar figurinha\",\n                    text: texto,\n                    url: url\n                })\n                    .then(() => console.log(\"Compartilhamento concluído\"))\n                    .catch((error) => console.error(\"Erro ao compartilhar:\", error));\n            } else {\n                this.showShareOptions = true;\n            }\n        },\n        copiaCola() {\n            const texto = `Eu tirei ${this.stickerText}, qual você vai tirar?`;\n            const url = window.location.href;\n\n            const tempInput = document.createElement(\"input\");\n            tempInput.value = `${texto} ${url}`;\n            document.body.appendChild(tempInput);\n            tempInput.select();\n            document.execCommand(\"copy\");\n            document.body.removeChild(tempInput);\n            alert(\"Link copiado para a área de transferência!\");\n\n            this.closeShare()\n        },\n    }\n}\n</script>"],"mappings":"AA2BA,eAAe;EACXA,OAAO,EAAE;IACLC,UAAUA,CAAA,EAAG;MACf,IAAI,CAACC,gBAAe,GAAI,KAAK;MAC7BC,OAAO,CAACC,GAAG,CAAC,eAAe;IAC7B,CAAC;IACGC,YAAYA,CAAA,EAAG;MACX,IAAI,CAAC,IAAI,CAACC,WAAW,EAAE;QACnBC,KAAK,CAAC,+CAA+C,CAAC;QACtD;MACJ;MAEA,MAAMC,KAAI,GAAI,YAAY,IAAI,CAACF,WAAW,wBAAwB;MAClE,MAAMG,GAAE,GAAIC,MAAM,CAACC,QAAQ,CAACC,IAAI;MAEhC,IAAIC,SAAS,CAACC,KAAI,IAAK,OAAOD,SAAS,CAACC,KAAI,KAAM,UAAU,EAAE;QAC1DD,SAAS,CAACC,KAAK,CAAC;UACZC,KAAK,EAAE,wBAAwB;UAC/BC,IAAI,EAAER,KAAK;UACXC,GAAG,EAAEA;QACT,CAAC,EACIQ,IAAI,CAAC,MAAMd,OAAO,CAACC,GAAG,CAAC,4BAA4B,CAAC,EACpDc,KAAK,CAAEC,KAAK,IAAKhB,OAAO,CAACgB,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC,CAAC;MACxE,OAAO;QACH,IAAI,CAACjB,gBAAe,GAAI,IAAI;MAChC;IACJ,CAAC;IACDkB,SAASA,CAAA,EAAG;MACR,MAAMZ,KAAI,GAAI,YAAY,IAAI,CAACF,WAAW,wBAAwB;MAClE,MAAMG,GAAE,GAAIC,MAAM,CAACC,QAAQ,CAACC,IAAI;MAEhC,MAAMS,SAAQ,GAAIC,QAAQ,CAACC,aAAa,CAAC,OAAO,CAAC;MACjDF,SAAS,CAACG,KAAI,GAAI,GAAGhB,KAAK,IAAIC,GAAG,EAAE;MACnCa,QAAQ,CAACG,IAAI,CAACC,WAAW,CAACL,SAAS,CAAC;MACpCA,SAAS,CAACM,MAAM,CAAC,CAAC;MAClBL,QAAQ,CAACM,WAAW,CAAC,MAAM,CAAC;MAC5BN,QAAQ,CAACG,IAAI,CAACI,WAAW,CAACR,SAAS,CAAC;MACpCd,KAAK,CAAC,4CAA4C,CAAC;MAEnD,IAAI,CAACN,UAAU,CAAC;IACpB;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}